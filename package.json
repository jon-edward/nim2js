{
  "name": "nim2js",
  "version": "1.0.0",
  "type": "module",
  "description": "Compile Nim source code to JS in the browser.",
  "main": "index.js",
  "files": [
    "pkg/nim2js.js",
    "pkg/nim2js.wasm"
  ],
  "scripts": {
    "build": "npm run build-release",
    "build-dev": "nim c -d:emscripten -d:debug nim/nim2js.nim",
    "build-release": "nim c -d:emscripten -d:release nim/nim2js.nim"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jon-edward/nim2js.git"
  },
  "keywords": [
    "Nim",
    "WASM"
  ],
  "author": "jon-edward",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jon-edward/nim2js/issues"
  },
  "homepage": "https://github.com/jon-edward/nim2js#readme"
}
